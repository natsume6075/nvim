# vim snippets

snippet     aiu
abbr        ok
regexp      '\(foo\|bar\)'
    yokuwakannnaine


snippet     language
options     head
    " ${1:#:lang} -------------------------
    autocmd initvim FileType ${2:#:lang}
    		\ let $LANG = "${3:#:lang}"

snippet     substitute
options     word
    substitute(${1:#:TARGET}, "${2:#:pattern}", "${2:#:string}", "g")

